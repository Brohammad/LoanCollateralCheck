# Google Gemini (Generative API)
GOOGLE_API_KEY=your-google-api-key
GOOGLE_API_PROJECT=your-google-project-id

# Model Configuration
GENERATION_MODEL=gemini-2.0-flash-exp
EMBEDDING_MODEL=text-embedding-004

# Generation Parameters
GENERATION_TEMPERATURE=0.7
CLASSIFICATION_TEMPERATURE=0.3
MAX_TOKENS=2048
SAFETY_SETTINGS=medium

# Pinecone (optional)
PINECONE_API_KEY=your-pinecone-key
PINECONE_ENV=us-west1-gcp
PINECONE_INDEX=ai-agent-workflow

# Chroma (optional)
CHROMA_PERSIST_DIR=./chroma_db

# SQLite
SQLITE_PATH=./data/credit_history.db

# Orchestrator Settings
CONFIDENCE_THRESHOLD=0.6
MAX_CRITIQUE_ITERATIONS=2

# Cache Settings
CACHE_TTL_SECONDS=3600

# Server
HOST=127.0.0.1
PORT=8000
